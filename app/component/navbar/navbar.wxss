.header{
  width: 100%;
  background-color: #fff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 50;
}
.header.on{
  background:linear-gradient(180deg,rgba(0,0,0,0.8) 0%,rgba(0,0,0,0) 100%);
}
.header .headBox{
  left: 0;
  top:0;
  width: 162rpx;
  padding-top: 11rpx;
  box-sizing: border-box;
  line-height: 50rpx;
  border:1px solid #ddd;
  border-radius: 50rpx;
  text-align: center;
  position: relative;
  z-index: 10;
  background: #d9d9d9;
}
.header.on .headBox{
   background: rgba(255,255,255,0.1)
}

.header .headBox:before{
  display: block;
  content: '';
  width: 2rpx;
  height: 40rpx;
  background-color: #E9E9E9;
  position: absolute;
  left: 49%;
  top: 50%;
  margin-top: -20rpx;
}

.header .headBox .arr{
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  margin: 0 18rpx;
}
.header .navtitle{
  text-align: center;
  font-size: 34rpx;
  color: #000;
  position: absolute;
  left: 0;
  width: 100%;
}
.header.on .navtitle{
  color: #fff;
}
.fox-bottom-bg{
  width:100%;
  height:100%;
  background:rgba(0,0,0,1);
  opacity:0.65;
  position:fixed;
  left: 0;
  top: 0;
  z-index: 51;
}
.fox-box{
  width: 350rpx;
  height: 100%;
  background-color: #fff;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 52;
}

.header .headBox1{
  width: 66rpx;
  border-radius: 50%;
}
.header .headBox1:before{
  display: none;
}
.header .headBox1 .arr {
  margin: 0 10rpx;
}
